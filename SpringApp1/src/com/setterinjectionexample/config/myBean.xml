<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
<bean id="addressObj" class="com.setterinjection.bean.Address">
<property name="state"  value="TS"/>
<property name="city" value="Hyderabad"/>



</bean>
<bean id="studentObj" class="com.setterinjection.bean.Student">
<property name="no"  value="1001"/>
<property name="name" value="Dharmendra"/>
<property name="address" ref="addressObj"/>



</bean>

</beans>